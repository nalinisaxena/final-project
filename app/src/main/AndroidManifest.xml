<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.nalini.navi1">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>



        <activity
            android:name="com.example.nalini.navi1.MainActivity.Add"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="com.example.nalini.navi.another.smart_shipment_service.ANOTHER_ACTIVITY" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>

        <activity
            android:name="com.example.nalini.navi1.Add"
            android:label="@string/title_activity_add"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name="com.example.nalini.navi1.DataListActivity"
            android:label="@string/title_activity_data_list"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name="com.example.nalini.navi1.SearchContactActivity"
            android:label="@string/title_activity_search_contact"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name="com.example.nalini.navi1.UpdateContactActivity"
            android:label="@string/title_activity_update_contact"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name="com.example.nalini.navi1.Admin"
            android:label="@string/title_activity_admin"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name="com.example.nalini.navi1.Salesman"
            android:label="@string/title_activity_salesman"
            android:theme="@style/AppTheme.NoActionBar" />








    </application>






</manifest>